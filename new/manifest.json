{
  "name": "FNG Status Plugin 2.0",
  "version": "0.2.0",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.com",
  "homepage_url": "http://fearnixx.de",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "page_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "page action demo",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "contextMenus",
    "cookies",
    "tabs",
    "geolocation",
    "https://fearnixx.de/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://google.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}