{
    "name": "FNG Status Plugin",
    "version": "2.1.1",
    "manifest_version": 3,
    "description": "A Extension created for the FearNixx Community to monitor the uptime status.",
    "homepage_url": "http://fearnixx.de",
    "content_scripts": [{
        "matches": ["https://dev.status.fearnixx.de/*"],
        "css": [
            "css/app.css"
        ],
        "js": [
            "js/app.js"
        ]
    }],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_icon": "icons/icon16.png",
        "default_title": "FNG Status Plugin",
        "default_popup": "popup.html"
    },
    "permissions": [
    ],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens index.html"
        }
    }
}